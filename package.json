{
  "name": "monkechu",
  "version": "1.0.8",
  "description": "Completely hackable and editable without repackaging client for iMonke",
  "main": "index.js",
  "dependencies": {
    "electron": "^3.0.6",
    "electron-packager": "^15.1.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^6.0.1",
    "imonke": "^0.3.5"
  },
  "devDependencies": {},
  "scripts": {
    "start": "electron .",
    "build-linux": "electron-packager . iMonke  --overwrite --prune --platform linux --arch x64 --icon=src/monkeshare/imonke.png --out builds/linux/",
    "build-mac": "electron-packager . iMonke --overwrite --platform=darwin --arch=x64 --icon=src/monkeshare/imonke.png --prune=true --out=builds/mac/",
    "build-win": "electron-packager . iMonke --overwrite --asar=true --platform=win32 --arch=ia32 --icon=src/monkeshare/imonke.png  --prune=true --out=builds/windows --version-string.CompanyName=iMonke'iMonke' --version-string.FileDescription=CE --version-string.ProductName='iMonke'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TobiWtf/Monkechu.git"
  },
  "author": "Tobi",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/TobiWtf/Monkechu/issues"
  },
  "homepage": "https://github.com/TobiWtf/Monkechu#readme",
  "keywords": []
}
